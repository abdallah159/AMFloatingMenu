<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/floatingBaseView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="ltr"
    android:orientation="vertical">

    <View
        android:id="@+id/floatingCL"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <TextView
        android:id="@+id/settingsTV"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/floating_border_item"
        android:gravity="center"
        android:layoutDirection="ltr"
        android:text="Settings"
        android:textColor="@color/whiteColor"
        android:textDirection="ltr"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.472" />

    <ImageView
        android:id="@+id/floatingActionButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layoutDirection="ltr"
        android:textDirection="ltr"
        app:backgroundTint="#601630"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.954"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976"
        app:srcCompat="@drawable/ic_magrabi_floating" />

    <TextView
        android:id="@+id/bookingTV"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/floating_border_item"
        android:gravity="center"
        android:layoutDirection="ltr"
        android:text="Booking"
        android:textColor="@color/whiteColor"
        android:textDirection="ltr"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.479" />

    <TextView
        android:id="@+id/findDoctorsTV"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/floating_border_item"
        android:gravity="center"
        android:layoutDirection="ltr"
        android:text="Find Docor"
        android:textColor="@color/whiteColor"
        android:textDirection="ltr"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <TextView
        android:id="@+id/locationsTV"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/floating_border_item"
        android:gravity="center"
        android:layoutDirection="ltr"
        android:text="Locations"
        android:textColor="@color/whiteColor"
        android:textDirection="ltr"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <TextView
        android:id="@+id/homeTV"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/floating_border_item"
        android:gravity="center"
        android:layoutDirection="ltr"
        android:text="@string/home"
        android:textColor="@color/whiteColor"
        android:textDirection="ltr"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <TextView
        android:id="@+id/servicesTV"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/floating_border_item"
        android:gravity="center"
        android:layoutDirection="ltr"
        android:text="Service"
        android:textColor="@color/whiteColor"
        android:textDirection="ltr"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <TextView
        android:id="@+id/blogTV"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/floating_border_item"
        android:gravity="center"
        android:layoutDirection="ltr"
        android:text="Blogs"
        android:textColor="@color/whiteColor"
        android:textDirection="ltr"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="78dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
